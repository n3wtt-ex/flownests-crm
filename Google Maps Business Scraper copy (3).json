    {
      "parameters": {
        "jsCode": "const item = $input.item.json;\n\n// Regex'ler\nconst emailRegex = /^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/;\n// Telefon Regex'i '+' karakterini de kabul ediyor\nconst phoneRegex = /^[\\d\\s\\(\\)\\-\\+]+$/;\n\nlet finalEmail = '';\nlet finalPhone = '';\n\n// 1. E-posta Se√ßimi ve Doƒürulamasƒ±\nif (item.email && typeof item.email === 'string' && emailRegex.test(item.email)) {\n  finalEmail = item.email;\n} else if (item.mail && typeof item.mail === 'string' && emailRegex.test(item.mail)) {\n  finalEmail = item.mail;\n}\n\n// 2. Telefon Se√ßimi, √ñnceliklendirme ve Doƒürulama\nlet internationalPhone = null;\nlet localPhone = null;\n\nif (item.tel && typeof item.tel === 'string' && phoneRegex.test(item.tel)) {\n  if (item.tel.startsWith('+')) {\n    internationalPhone = item.tel;\n  } else {\n    localPhone = item.tel;\n  }\n}\n\nif (item.phone && typeof item.phone === 'string' && phoneRegex.test(item.phone)) {\n  if (item.phone.startsWith('+')) {\n    internationalPhone = item.phone; // Yeni bulunan uluslararasƒ±, eskisinin √ºzerine yazar\n  } else if (!localPhone) { // Sadece ba≈üka bir yerel numara yoksa ata\n    localPhone = item.phone;\n  }\n}\n\nfinalPhone = internationalPhone || localPhone || '';\n\n// 3. Sayƒ±sal alanlarƒ± temizle\nif (item.reviews && typeof item.reviews === 'string') {\n  const reviewCount = parseInt(item.reviews.trim(), 10);\n  item.reviews = isNaN(reviewCount) ? null : reviewCount;\n} else if (typeof item.reviews !== 'number') {\n  item.reviews = null;\n}\n\nif (item.rating && typeof item.rating === 'string') {\n  const ratingValue = parseFloat(item.rating.trim());\n  item.rating = isNaN(ratingValue) ? null : ratingValue;\n} else if (typeof item.rating !== 'number') {\n  item.rating = null;\n}\n\n// 4. Sonu√ßlarƒ± Baserow'a uygun alan adlarƒ±na ata\nitem.mail = finalEmail;\nitem.tel = finalPhone;\n\n// Artƒ±k ihtiya√ß duyulmayan eski/ge√ßici alanlarƒ± sil\ndelete item.email;\ndelete item.phone;\n\nreturn item;"
      },
