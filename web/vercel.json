{
  "version": 2,
  "routes": [
    {
      "src": "^/robots.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "^/sitemap.xml$",
      "dest": "/sitemap.xml"
    }
  ],
  "redirects": [
    {
      "source": "https://www.flownests.org/(.*)",
      "destination": "https://flownests.org/$1",
      "permanent": true
    }
  ]
}
